{
  "name": "cypressdemo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",

    "clean:reports": "if exist cypress\\reports rmdir /s /q cypress\\reports",

    "test:report": "npm run clean:reports && cypress run --browser chrome",

    "merge-reports": "mochawesome-merge \"cypress/reports/**/.jsons/**/*.json\" > cypress/reports/merged/merged.json",
    "generate-report": "marge cypress/reports/merged/merged.json -f index -o cypress/reports/merged",

    "report:merged": "npm run merge-reports && npm run generate-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
    "cypress": "^15.9.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^4.0.2",
    "esbuild": "^0.27.2",
    "mocha": "^11.7.5",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.0",
    "mochawesome-report-generator": "^6.3.2"
  }
}
